    "# Calculate and print precision, recall, and mAP\n",
    "precision = np.mean([np.mean(p) for p in precisions])\n",
    "recall = np.mean([np.mean(r) for r in recalls])\n",
    "mAP = np.mean(APs)\n",
    "\n",
    "print(f\"Precision: {precision}\")\n",
    "print(f\"Recall: {recall}\")\n",
    "print(f\"mAP: {mAP}\")"
